mixin user_suggestion(data)
  div.card
    div.card-header.flex.justify-between.items-center.gap-1
      h6(class="font-bold dark:text-white") Suggestions
      label.btn.bg-gray-light.p-1.cursor-pointer(for='suggest-modal' class="dark:bg-sidebar-dark2") 
        i(data-feather="plus" class="w-4 h-4 stroke-title dark:stroke-white ") 
    div.card-body
      ul.flex.flex-col.gap-3
        each item in data
          li.flex.justify-between.items-center.suggest-friend
            div.flex.items-center.gap-2.w-full
              div.avatar.rounded-full(class="w-10 h-10")
                img(src=`../../assets/images/user-app/${item.image}` alt=`${item.title}`)                                                      
              div(class="w-[calc(100%_-_52px)]")
                p.text-title(class="text-xs font-semibold dark:text-white ")=item.userName
                span.text-content3(class="font-semibold text-2xs")=item.userProfile
            a(href="javascript:void(0)" class="dark:bg-sidebar-dark2 ").btn.btn-primary.btn-outline.p-1.group.suggest-btn
              +SVG_ICON("Profile", "w-4 h-4 stroke-primary group-hover:stroke-white ")

mixin user_friend(data)
  div.card
    div.card-header.flex.justify-between.items-center.gap-1 
      h6(class="font-bold dark:text-white") Friends
      label.btn.bg-gray-light.p-1.cursor-pointer(for='add-friend-modal' class="dark:bg-sidebar-dark2") 
        i(data-feather="plus" class="w-4 h-4 stroke-title dark:stroke-white")                     
    div.card-body
      ul.flex.flex-col.gap-3 
        each item in data
          li.flex.justify-between.items-center.add-friend
            div.flex.items-center.gap-2.w-full
              div.avatar.rounded-full(class="w-10 h-10")
                img(src=`../../assets/images/user-app/${item.image1}` alt=`${item.title1}`)                                                      
              div(class="w-[calc(100%_-_52px)]")
