                    div(data-tabContent="1" class="tab-pan active show first:rounded-tl-none rounded-5")
                      div.flex.flex-wrap.gap-2.justify-center
                        div.mask.mask-square
                          img(src='../../assets/images/avatar/20.jpg' alt='avatar')
