//- sidebar
-var sidebar = true;

//- feathet
-var feather= true;

//- customSelect
-var customSelect= true;

//- customizer
-var customizer=true;

//- prism
-var prism=true;

//- Breadcrumbs
-var pageTitle="Breadcrumb";
-var breadcrumb=['Bonus Ui','Breadcrumb'];

//- json
include ../../data/all-data

//- mixin
include ../../mixin/mixin

doctype html
html(lang='en')  
   include ../../layout/header-files 
   body(dir="ltr" class="unique-skin") 
      main.page-wrapper
         div.page-body-wrapper(class="2xl:w-full 4xl:w-[calc(100%_-_14.375rem)] w-[calc(100%_-_15.8125rem)] ml-auto transition-all duration-300")
            include ../../layout/header/header     
            // Sidebar Start 
            div.page-sidebar.sidebar-dark.unique-sidebar
               include ../../layout/unique
            // Sidebar End 

            // Page Body Start 
            div.page-body
               div.grid.grid-cols-12.gap-card-gap
                  div.col-span-6(class="2lg:col-span-12")
                     div.card.tabs
                        div.card-header.pb-2.header-bottom-border
                           div.flex.flex-col
                              h3.text-lg.font-bold Default Breadcrumb 
                              p   Adding breadcrumb using 
                                 strong .breadcrumb 
                                 |   class.
                              div.preview-code.tab-links
                                 button.p-0.tab.tab-lifted.tab-active(data-tabFilter="1" class="tab-link") Preview  
                                 button.p-0.tab.tab-lifted(data-tabFilter="2" class="tab-link tab-2") Html
                        div.card-body                                   
                           div.tabs-content
                              div(data-tabContent="1" class="tab-pan active show first:rounded-tl-none rounded-5")
                                 div(class="overflow-auto custom-scroll")
                                    div.breadcrumb
                                       nav
                                          ol.flex.items-center.breadcrumb-list 
                                             li.breadcrumb-items
                                                a(href="javascript:void(0)")
                                                   +SVG_ICON("Home","stroke-primary")
                                             li.px-2 
                                                span /
                                             li.breadcrumb-items.capitalize Bonus Ui 
                                             li.px-2 
                                                span /
                                             li.breadcrumb-items.capitalize.font-light.active Breadcrumb
                              div(data-tabContent="2" class="relative tab-pan copyParent group first:rounded-tl-none rounded-5")
                                 button.btn.copyBtn 
                                    i(data-feather="clipboard")
                                 pre(class="overflow-auto language-html custom-scroll")
                                    code
                                       |&lt;div class="overflow-auto custom-scroll"&gt;
                                       |    &lt;div class="breadcrumb"&gt;
                                       |     &lt;nav&gt;
                                       |      &lt;ol class="flex items-center breadcrumb-list"&gt;
                                       |        &lt;li class="breadcrumb-items"&gt;
                                       |          &lt;a href="javascript:void(0)"&gt; +SVG_ICON("Home","stroke-primary") &lt;/a&gt;
                                       |        &lt;/li&gt;
                                       |        &lt;li class="px-2"&gt;&lt;span&gt;/&lt;/span&gt;&lt;/li&gt;
                                       |        &lt;li class="capitalize breadcrumb-items"&gt;Bonus Ui&lt;/li&gt;
                                       |        &lt;li class="px-2"&gt;&lt;span&gt;/&lt;/span&gt;&lt;/li&gt;
                                       |        &lt;li class="font-light capitalize breadcrumb-items active"&gt;Breadcrumb&lt;/li&gt;
                                       |      &lt;/ol&gt;
                                       |     &lt;/nav&gt; 
                                       |    &lt;/div&gt;
                                       |&lt;/div&gt;
