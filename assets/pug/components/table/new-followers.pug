//- ========================
//-  New Followers
//- =======================
table.table.equal-space.table-sm 
   thead 
      tr 
         each head in NEW_FOLLOWERS.header
            th=  head
   tbody
      each bodyData in NEW_FOLLOWERS.body
         tr
            td(class="min-w-[238px]") 
               a.flex.items-center(href="./invoice.html" class="gap-[9px]")
                  img(class="2xl:w-[40px] 2xl:h-[40px] 3xl:w-[34px] 3xl:h-[34px] 5xl:w-[35px] 5xl:h-[35px] 6xl:w-[37px] 6xl:h-[37px] sm:w-[35px] sm:h-[35px] w-[43px] h-[43px] rounded-full" src=`../../assets/images/avatar/${bodyData.image}`, alt=`${bodyData.name}`) 
                  div
                     h4(class="text-xs font-semibold text-title leading-none")=bodyData.name
                     span(class="block text-2xs font-semibold mt-1 text-content leading-none")=bodyData.subTitle
            td(class="min-w-[120px]") 
               h4(class="text-xs font-semibold text-content")= bodyData.postLike  
            td(class="min-w-[140px]") 
               button.btn.bg-gray-light.text-title(class="px-[15px] py-[7px] 5xl:py-[5px] text-xs font-semibold")= bodyData.status 
            td(class="min-w-[50px] text-end")=`${bodyData.point}%` 

